{
  terrainReplace: {
    include: {
      "*:leavesbranchy-*" : 1,
      "*:leavesbranchystatic-*" : 1,
      "*:bambooleaves-*" : 1,
      "*:leaves-*" : 1,
      "*:log-*" : 1,
      "haven:branches-*": 1,
      "game:wildvine-*" : 1
    }
  },
  resourceZoneClear: {
    include: {
      "*:leavesbranchy-*" : 1,
      "*:leavesbranchystatic-*" : 1,
      "*:bambooleaves-*" : 1,
      "*:leaves-*" : 1,
      "*:log-*" : 1,
      "haven:branches-*": 1,
      // Clear the vines so that they are not left hanging after the trees are removed.
      "game:wildvine-*" : 1
    }
  }
}
